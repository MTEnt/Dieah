{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/schema.json",
  "productName": "Dieah",
  "version": "0.1.0",
  "identifier": "com.dieah.app",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../ui"
  },
  "bundle": {
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "Dieah",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "capabilities": ["main-capability"]
    }
  }
}
